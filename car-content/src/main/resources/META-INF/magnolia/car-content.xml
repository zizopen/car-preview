<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
    <name>car-content</name>
    <displayName>${project.name}</displayName>
    <description>${project.description}</description>
    <class>com.vsoft.car.content.CarContent</class>
    <versionHandler>com.vsoft.car.content.setup.CarContentVersionHandler</versionHandler>
    <version>
        ${parsedVersion.majorVersion}.${parsedVersion.minorVersion}.${parsedVersion.incrementalVersion}-${git.buildnumber}
    </version>

    <dependencies>
        <dependency>
            <name>core</name>
            <version>5.3.8/*</version>
        </dependency>
        <!-- Add other dependencies here, e.g the standard-templating-kit, or the multisite module with the enterprise edition. -->
        <!--
          <dependency>
            <name>standard-templating-kit</name>
            <version>2.7/*</version>
          </dependency>
          <dependency>
            <name>multisite</name>
            <version>1.0/*</version>
          </dependency>
        -->
    </dependencies>
    <repositories>
        <repository>
            <name>magnolia</name>
            <workspaces>
                <workspace>car</workspace>
            </workspaces>
            <nodeTypeFile>/mgnl-nodetypes/magnolia-car-nodetypes.xml</nodeTypeFile>
        </repository>
    </repositories>
</module>